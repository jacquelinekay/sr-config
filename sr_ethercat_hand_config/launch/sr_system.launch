<launch>
  <arg name="gui" default="true"/>
  <arg name="initial_z" default="0.0"/>
  <include file="$(find sr_ethercat_hand_config)/launch/sr_rosbag_log.launch">
    <arg name="log_bag_prefix" value="sr_hand_and_arm_log"/>
  </include>
  <include file="$(find sr_robot_launch)/launch/sr_right_ur10arm_hand.launch">
    <arg name="eth_port" value="enp4s0"/>
    <arg name="hand_serial" value="1802" />

    <arg name="robot_description" value="$(find sr_multi_description)/urdf/right_srhand_stand_ur10_bt_sp_biotacs.urdf.xacro" />
    <arg name="use_moveit" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="mapping_path" value="$(find sr_edc_launch)/mappings/default_mappings/rh_E_v3.yaml"/>
    <arg name="sim" value="false"/>
  </include>

</launch>
